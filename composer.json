{
    "name": "phine/compact",
    "description": "A PHP library for compacting the contents of files.",
    "keywords": ["compact"],
    "homepage": "https://github.com/phine/lib-compact",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Herrera",
            "email": "kevin@herrera.io",
            "homepage": "http://kevin.herrera.io"
        }
    ],
    "support": {
        "issues": "https://github.com/phine/lib-compact/issues"
    },
    "require": {
        "php": ">=5.3.3",
        "phine/exception": "~1.0"
    },
    "require-dev": {
        "league/phpunit-coverage-listener": "~1.0",
        "phine/test": "~1.0"
    },
    "suggest": {
        "ext-dom": "For compacting XML files.",
        "ext-json": "For compacting JSON files."
    },
    "autoload": {
        "psr-0": {
            "Phine\\Compact": "src/lib"
        }
    },
    "config": {
        "bin-dir": "bin",
        "vendor-dir": "src/vendors"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
